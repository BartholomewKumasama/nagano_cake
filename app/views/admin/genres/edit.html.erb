<div class="row">
  <div class="col-md">
    <h2>ジャンル編集</h2>
     <tbody>
      <%= form_with model: @genre, url: admin_genre_path do |f| %>
        <div class="table">
          <tr>
            <td><label for="name">ジャンル名</label></td>
            <td>
              <%= f.text_field :name %>
            </td>
            <td>
              <%= f.submit "変更を保存", class: "btn btn-success" %>
            </td>
          </tr>
        </div>
        <% end %>
     </tbody>
  </div>
</div>